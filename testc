import requests

# Authentication details
username = 'your_username'
password = 'your_password'
base_url = 'https://your-confluence-domain.com/rest/api/content'

# Page ID of the page you want to update
page_id = '123456789'

# New content for the page
new_content = """
<h1>New Content</h1>
<p>This is the updated content of the page.</p>
"""

# Prepare the data to send
data = {
    "id": page_id,
    "type": "page",
    "title": "Updated Page Title",
    "body": {
        "storage": {
            "value": new_content,
            "representation": "storage"
        }
    }
}

# Make the request to update the page
response = requests.put(f"{base_url}/{page_id}", json=data, auth=(username, password))

# Check if the request was successful
if response.status_code == 200:
    print("Page updated successfully!")
else:
    print(f"Failed to update page. Status code: {response.status_code}")
    print(response.text)
